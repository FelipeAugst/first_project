{
    "openapi": "3.0.0",
    "info": {
        "title": "First Laravel API",
        "description": "Simple Laravel Api who manage users and associated profiles",
        "version": "0.0.1"
    },
    "paths": {
        "/api/profile/index": {
            "get": {
                "tags": [
                    "Profile"
                ],
                "summary": "List all profiles.",
                "operationId": "69bf9ef74089f011e004a0017c4ddf11",
                "parameters": [
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "limit of results returned",
                        "required": true,
                        "schema": {
                            "type": "int"
                        }
                    },
                    {
                        "name": "orderby",
                        "in": "query",
                        "description": "column to order data",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "direction",
                        "in": "query",
                        "description": "direction of data ordenation",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/profile"
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "0": "bearerAuth:{}"
                    }
                ]
            }
        },
        "/api/profile/store": {
            "post": {
                "tags": [
                    "Profile"
                ],
                "summary": "Create a new profile",
                "operationId": "f8d0fdc840e2f4f6572e656002f37dde",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "allOf": [
                                    {
                                        "$ref": "#components/schemas/profile"
                                    }
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#components/schemas/profile"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "422": {
                        "description": "Unprocessable Content"
                    }
                },
                "security": [
                    {
                        "0": "bearerAuth:{}"
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "profile": {
                "title": "profile",
                "description": "profile schema",
                "required": [
                    "user_id",
                    "profile"
                ],
                "properties": {
                    "user_id": {
                        "description": "Id of the user who own the profile",
                        "type": "integer",
                        "format": "int32"
                    },
                    "profile": {
                        "description": "Type of profile",
                        "type": "string",
                        "format": "string"
                    }
                },
                "type": "object"
            }
        },
        "responses": {
            "401": {
                "description": "Unauthorized"
            },
            "422": {
                "description": "unprocessable content"
            }
        }
    },
    "tags": [
        {
            "name": "Profile",
            "description": "Profile"
        }
    ]
}